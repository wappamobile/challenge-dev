services:
  challenge.api:
    build:
      context: C:\Users\Windows\source\repos\challenge-dev\Challenge
      dockerfile: Challenge.Api/Dockerfile
    depends_on:
    - mongodb
    environment:
      ASPNETCORE_ENVIRONMENT: Development
    image: challengeapi
    ports:
    - target: 80
  mongodb:
    image: mongo:latest
    ports:
    - published: 27017
      target: 27017
version: '3.4'